{
  "name": "kenneth-s-cv",
  "version": "1.0.0",
  "description": "Web design of my CV",
  "scripts": {
    "dev:client": "webpack --watch --config client/webpack.config.js",
    "dev:server": "nodemon --watch server",
    "client": "webpack --config client/webpack.config.js",
    "dev": "run-p dev:client dev:server",
    "knex": "knex --knexfile server/db/knexfile.js",
    "webpack:server": "webpack-dev-server --hot --inline --content-base server/public --config client/webpack.config.js",
    "test": "jest --verbose",
    "test:watch": "jest --watchAll --verbose",
    "test:e2e": "DEBUG=pw:api NODE_ENV=e2e jest",
    "test:all": "npm run lint && npm run test && npm run test:e2e",
    "server": "node server",
    "start": "node server",
    "lint": "eslint --ext .js,.jsx client server",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "db:reset": "npm run knex migrate:rollback && npm run db:migrate && npm run db:seed",
    "build": "run-s client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Piecre24/Kenneth-s-CV.git"
  },
  "eslintConfig": {
    "extends": "eda",
    "root": true,
    "ignorePatterns": [
      "bundle.js"
    ]
  },
  "engines": {
    "node": ">=12.0"
  },
  "keywords": [
    "react",
    "boilerplate"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Piecre24/Kenneth-s-CV/issues"
  },
  "homepage": "https://github.com/Piecre24/Kenneth-s-CV#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^1.0.6",
    "authenticare": "^0.4.7",
    "aws-amplify": "^3.3.26",
    "cors": "^2.8.5",
    "d": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "knex": "^0.21.12",
    "pg": "^8.5.1",
    "playwright-cli": "^0.162.1",
    "superagent": "^5.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@babel/register": "^7.10.3",
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^5.11.0",
    "@testing-library/react": "^10.4.3",
    "@testing-library/user-event": "^12.0.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "7.13.0",
    "eslint-config-eda": "^0.1.4",
    "jest": "^26.1.0",
    "leaflet": "^1.7.1",
    "msw": "^0.19.5",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "playwright": "^1.7.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-leaflet": "^3.0.2",
    "react-material-ui-carousel": "^2.1.1",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^4.2.0",
    "supertest": "^4.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
